<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Alternate Text Image"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <title
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Title"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <toptext
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Enter Top Slider Text"
                                        name="./topslidertext"/>
                                    <bottomtext
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Enter Bottom Slider Text"
                                        name="./bottomslidertext"/>
                                </items>
                            </columns>
                        </items>
                    </title>
                    <top
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Top"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <title
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Enter Title"
                                        name="./topTitle"/>
                                    <heading
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Enter Heading"
                                        name="./topHeading"/>
                                    <image
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                        allowUpload="false"
                                        class="cq-droptarget"
                                        fieldLabel="Image"
                                        fileNameParameter="./fileNameImage"
                                        fileReferenceParameter="./topFileReferenceImage"
                                        mimeTypes="[image]"
                                        multiple="{Boolean}false"
                                        name="./image"
                                        title="Drop Image Here"
                                        useHTML5="{Boolean}true"/>
                                    <imagealt
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Enter Alternate Text"
                                        name="./topImagealttext"/>
                                </items>
                            </columns>
                        </items>
                    </top>
                    <center
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Center"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <title
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Enter Title"
                                        name="./centerTitle"/>
                                    <heading
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Enter Heading"
                                        name="./centerHeading"/>
                                    <description
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                        fieldLabel="Enter Description"
                                        name="./centerDescription"
                                        removeSingleParagraphContainer="{Boolean}true"
                                        useFixedInlineToolbar="{Boolean}true">
                                        <rtePlugins
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceSuperType="/apps/cision-revamp/components/common/customrte/cq:dialog/content/items/column/items/text/rtePlugins"/>
                                        <uiSettings
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceSuperType="/apps/cision-revamp/components/common/customrte/cq:dialog/content/items/column/items/text/uiSettings"/>
                                    </description>
                                    <buttontitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Enter button Title"
                                        name="./centerButtontitle"/>
                                    <buttonLink
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                        fieldLabel="Button URL"
                                        name="./centerButtonLink"
                                        rootPath="/content"/>
                                    <image
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                        allowUpload="false"
                                        class="cq-droptarget"
                                        fieldLabel="Image"
                                        fileNameParameter="./fileNameImage"
                                        fileReferenceParameter="./centerFileReferenceImage"
                                        mimeTypes="[image]"
                                        multiple="{Boolean}false"
                                        name="./image"
                                        title="Drop Image Here"
                                        useHTML5="{Boolean}true"/>
                                    <imagealt
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Enter Alternate Text"
                                        name="./centerImagealttext"/>
                                </items>
                            </columns>
                        </items>
                    </center>
                    <bottom
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Bottom"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <title
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Enter Title"
                                        name="./bottomTitle"/>
                                    <heading
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Enter Heading"
                                        name="./bottomHeading"/>
                                    <description
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                        fieldLabel="Enter description"
                                        name="./bottomDescription"
                                        removeSingleParagraphContainer="{Boolean}true"
                                        useFixedInlineToolbar="{Boolean}true">
                                        <rtePlugins
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceSuperType="/apps/cision-revamp/components/common/customrte/cq:dialog/content/items/column/items/text/rtePlugins"/>
                                        <uiSettings
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceSuperType="/apps/cision-revamp/components/common/customrte/cq:dialog/content/items/column/items/text/uiSettings"/>
                                    </description>
                                    <buttontitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Enter button Title"
                                        name="./bottomButtontitle"/>
                                    <buttonLink
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                        fieldLabel="Button URL"
                                        name="./bottomButtonLink"
                                        rootPath="/content"/>
                                    <image
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                        allowUpload="false"
                                        class="cq-droptarget"
                                        fieldLabel="Image"
                                        fileNameParameter="./fileNameImage"
                                        fileReferenceParameter="./bottomFileReferenceImage"
                                        mimeTypes="[image]"
                                        multiple="{Boolean}false"
                                        name="./image"
                                        title="Drop Image Here"
                                        useHTML5="{Boolean}true"/>
                                    <imagealt
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="enter Alternate Text"
                                        name="./bottomImagealttext"/>
                                </items>
                            </columns>
                        </items>
                    </bottom>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
