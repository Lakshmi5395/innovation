<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Case Study Detail Element"
          extraClientlibs="clientlibs-multifieldValidation"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <fixedcolumns
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <tabs
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/tabs">
                        <items jcr:primaryType="nt:unstructured">
                            <leftArea
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Left Area"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <columns
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <introOverline
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Introduction Overline Text"
                                                name="./introOverline"/>
                                            <introTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Introduction Overline Title"
                                                name="./introTitle"/>
                                            <introDesc
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                fieldLabel="Introduction text medium Description"
                                                name="./introDesc"
                                                useFixedInlineToolbar="{Boolean}true">
                                                <rtePlugins
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="/apps/cision-revamp/components/common/customrte/cq:dialog/content/items/column/items/text/rtePlugins"/>
                                                <uiSettings
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="/apps/cision-revamp/components/common/customrte/cq:dialog/content/items/column/items/text/uiSettings"/>
                                            </introDesc>
                                            <headlineSeparator
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/heading"
                                                text="Add Headline Details "/>
                                            <headlineOverline
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Headline Overline Text"
                                                name="./headlineOverline"/>
                                            <headlineTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Headline Overline Title"
                                                name="./headlineTitle"/>
                                            <headlineDesc
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                fieldLabel="Headline text medium Description"
                                                name="./headlineDesc"
                                                removeSingleParagraphContainer="{Boolean}true"
                                                useFixedInlineToolbar="{Boolean}true">
                                                <rtePlugins
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="/apps/cision-revamp/components/common/customrte/cq:dialog/content/items/column/items/text/rtePlugins"/>
                                                <uiSettings
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="/apps/cision-revamp/components/common/customrte/cq:dialog/content/items/column/items/text/uiSettings"/>
                                            </headlineDesc>
                                        </items>
                                    </columns>
                                </items>
                            </leftArea>
                            <rightArea
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Right Area"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <fields
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                        aem-nested=""
                                        class="full-width"
                                        composite="{Boolean}true"
                                        fieldDescription="Click '+' to add a new field"
                                        fieldLabel="Elements"
                                        name="./rightElements"
                                        validation="multi-validate">
                                        <granite:data
                                                jcr:primaryType="nt:unstructured"
                                                fieldlimit="2"/>
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            jcr:title="Add New Case Study Detail Element"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                            name="./items">
                                            <items jcr:primaryType="nt:unstructured">
                                                <column
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <rightIntroOverline
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldLabel="Right Area Introduction Overline Text"
                                                            name="./rightIntroOverline"/>
                                                        <rightIntroTitle
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldLabel="Right Area Introduction Overline Title"
                                                            name="./rightIntroTitle"/>
                                                        <rightIntroDesc
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                            fieldLabel="Right Area Introduction text medium Description"
                                                            name="./rightIntroDesc"
                                                            removeSingleParagraphContainer="{Boolean}true"
                                                            useFixedInlineToolbar="{Boolean}true">
                                                            <rtePlugins
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceSuperType="/apps/cision-revamp/components/common/customrte/cq:dialog/content/items/column/items/text/rtePlugins"/>
                                                            <uiSettings
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceSuperType="/apps/cision-revamp/components/common/customrte/cq:dialog/content/items/column/items/text/uiSettings"/>
                                                        </rightIntroDesc>
                                                        <image
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                            allowUpload="false"
                                                            class="image-show cq-droptarget"
                                                            fieldLabel="Image"
                                                            fileNameParameter="./fileNameImage"
                                                            fileReferenceParameter="./fileReferenceGenericImage"
                                                            mimeTypes="[image]"
                                                            multiple="{Boolean}false"
                                                            name="./genericImage"
                                                            rel="image-show"
                                                            title="Drop Image"
                                                            useHTML5="{Boolean}true"/>
                                                        <imageAlt
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldLabel="Image Alternate Text"
                                                            name="./genericImageAlt"/>
                                                        <line
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/heading"
                                                            text="-------------------------------------------------------------"/>
                                                    </items>
                                                </column>
                                            </items>
                                        </field>
                                    </fields>
                                </items>
                            </rightArea>
                        </items>
                    </tabs>
                </items>
            </fixedcolumns>
        </items>
    </content>
</jcr:root>
